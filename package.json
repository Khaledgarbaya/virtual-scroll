{
  "name": "virtual-scroll-list",
  "version":"0.7.0",
  "description": "Think of it as UITableView for the DOM elements",
  "main": "webpack.config.js",
  "scripts": {
    "prepublish": "in-publish && npm run build || not-in-publish",
    "postpublish": "npm run clean",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdirp dist && babel src --out-dir dist",
    "build:umd": "webpack src/VirtualScroll.js dist/virtual-scroll.js && NODE_ENV=production webpack src/VirtualScroll.js dist/virtual-scroll.min.js",
    "lint": "eslint src",
    "test": "NODE_ENV=test mocha --compilers js:babel-register",
    "test:watch": "NODE_ENV=test mocha --compilers js:babel-register --watch",
    "test:cov": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Khaledgarbaya/virtual-scroll.git"
  },
  "keywords": [
    "virtual-scroll"
  ],
  "author": "Khaled Garbaya <kgarbaya@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.3.17",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "cz-conventional-changelog": "^1.1.6",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.0",
    "expect": "^1.6.0",
    "in-publish": "^2.0.0",
    "isparta": "^3.0.3",
    "mocha": "^2.3.4",
    "mock-browser": "^0.92.11",
    "rimraf": "^2.5.2",
    "semantic-release": "^4.3.5",
    "semantic-release-cli": "^1.4.1",
    "webpack": "^1.9.6",
    "webpack-dev-server": "^1.8.2"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "invariant": "^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
